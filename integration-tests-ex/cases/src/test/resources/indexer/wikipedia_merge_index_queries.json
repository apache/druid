[
    {
        "description": "groupby, stringFirst/stringLast rollup aggs, all",
        "query":{
            "queryType" : "groupBy",
            "dataSource": "%%DATASOURCE%%",
            "granularity":"day",
            "dimensions":[
                "continent"
            ],
            "intervals":[
                "2013-08-31T00:00/2013-09-01T00:00"
            ],
            "filter":{
                "type":"selector",
                "dimension":"continent",
                "value":"Asia"
            },
            "aggregations":[
                {
                    "type": "stringFirst",
                    "name": "earliest_user",
                    "fieldName": "first_user"
                },
                {
                    "type":"stringLast",
                    "name":"latest_user",
                    "fieldName":"last_user"
                }
            ]
        },
        "expectedResults":[ {
            "version" : "v1",
            "timestamp" : "2013-08-31T00:00:00.000Z",
            "event" : {
                "continent":"Asia",
                "earliest_user":"masterYi",
                "latest_user":"stringer"
            }
        } ]
    }
]