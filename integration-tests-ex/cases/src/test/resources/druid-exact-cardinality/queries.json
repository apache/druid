[
  {
    "description": "Timeseries query on non-rolled-up columns",
    "query": {
      "queryType": "timeseries",
      "dataSource": {
        "type": "union",
        "dataSources": [
          "%%DATASOURCE%%-1", "%%DATASOURCE%%-2", "%%DATASOURCE%%-3",
          "%%DATASOURCE%%-0"
        ]
      },
      "intervals": ["2013-08-31/2013-09-01"],
      "granularity": "all",
      "aggregations": [
        {
          "type": "count",
          "name": "added_count",
          "fieldName": "added"
        },
        {
          "type": "Bitmap64ExactCardinalityBuild",
          "name": "added_cardinality",
          "fieldName": "added"
        }
      ]
    },
    "expectedResults": [
      {
        "timestamp": "2013-08-31T01:02:33.000Z",
        "result": {
          "added_count": 15,
          "added_cardinality": 4
        }
      }
    ]
  },
  {
    "description": "Bitmap64 works on time column",
    "query": {
      "queryType": "timeseries",
      "dataSource": {
        "type": "union",
        "dataSources": [
          "%%DATASOURCE%%-1", "%%DATASOURCE%%-2", "%%DATASOURCE%%-3",
          "%%DATASOURCE%%-0"
        ]
      },
      "intervals": ["2013-08-31/2013-09-01"],
      "granularity": "all",
      "aggregations": [
        {
          "type": "count",
          "name": "time_count"
        },
        {
          "type": "Bitmap64ExactCardinalityBuild",
          "name": "time_cardinality",
          "fieldName": "__time"
        }
      ]
    },
    "expectedResults": [
      {
        "timestamp": "2013-08-31T01:02:33.000Z",
