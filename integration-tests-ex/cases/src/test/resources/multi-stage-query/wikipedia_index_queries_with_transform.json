[
    {
        "description":"having spec on post aggregation",
        "query":{
            "queryType":"groupBy",
            "dataSource":"%%DATASOURCE%%",
            "granularity":"day",
            "dimensions":[
                "page",
                "city"
            ],
            "filter":{
                "type":"selector",
                "dimension":"language",
                "value":"language-zh"
            },
            "aggregations":[
                {
                    "type":"count",
                    "name":"rows"
                },
                {
                    "type":"longSum",
                    "fieldName":"triple-added",
                    "name":"added_count"
                },
                {
                    "type":"longSum",
                    "fieldName":"delta",
                    "name":"delta_sum"
                }
            ],
            "postAggregations": [
                {
                    "type":"arithmetic",
                    "name":"added_count_times_ten",
                    "fn":"*",
                    "fields":[
                        {"type":"fieldAccess", "name":"added_count", "fieldName":"added_count"},
                        {"type":"constant", "name":"const", "value":10}
                    ]
                }
            ],
            "having":{"type":"greaterThan", "aggregation":"added_count_times_ten", "value":9000},
            "intervals":[
                "2013-08-31T00:00/2013-09-01T00:00"
            ]
        },
        "expectedResults":[ {
            "version" : "v1",
            "timestamp" : "2013-08-31T00:00:00.000Z",
            "event" : {
                "added_count_times_ten" : 27150.0,
                "page" : "Crimson Typhoon",
                "city" : "Taiyuan",
                "added_count" : 2715,
                "delta_sum" : 900,
                "rows" : 1
            }
        } ]
    }
]