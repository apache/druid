{
  "datacenter": "dc1",
  "data_dir": "/consul/data",
  "log_level": "INFO",
  "server": true,
  "bootstrap_expect": 1,
  "ui": true,
  "client_addr": "0.0.0.0",
  "ports": {
    "http": -1,
    "https": 8501,
    "grpc": -1,
    "grpc_tls": -1
  },
  "ca_file": "/tls/ca-cert.pem",
  "cert_file": "/tls/consul-server-cert.pem",
  "key_file": "/tls/consul-server-key.pem",
  "verify_incoming": true,
  "verify_outgoing": true,
  "verify_server_hostname": false
}

