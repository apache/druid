#-------------------------------------------------------------------------
# TESTCASE: test002 TEST_ID: A50
#-------------------------------------------------------------------------
SELECT count(*) cnt
FROM
  (SELECT repeat(string_agg(DISTINCT tab0d0, '|'), 2) AS col
   FROM test_agg_funcs
   WHERE tab0d0>111
     AND tab0d0<222)
WHERE col != 'ABCD'
  OR col IS NULL;
+-----+
| cnt |
+-----+
|   1 |
+-----+
(1 row)

!ok

