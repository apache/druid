#-------------------------------------------------------------------------
# TESTCASE: test_join TEST_ID: A2_B19
#-------------------------------------------------------------------------
SELECT t1.c_str,
       t1.c_long,
       c
FROM test_unnest t1,
     unnest(mv_to_array(c_mv_num_with_null_and_empty_val)) AS u(c)
JOIN test_unnest,
     unnest(mv_to_array(c_mv_num_with_null_and_empty_val)) t2 AS u(c) ON t1.c=t2.c
WHERE t1.c_long IN (0,
                    1,
                    2,
                    999)
  AND t2.c_long IN (0,
                    1,
                    2,
                    999);
