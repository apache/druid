SELECT c
FROM
  (SELECT *
   FROM test_unnest,
        unnest(mv_to_array(c_mv_num_with_null_and_empty_row)) AS u(c))
WHERE c_long IN (0,
                 1,
                 2) IS NOT FALSE
  AND c_long IN (3,
                 4) IS FALSE
  AND c IN ('',
            'my_null',
            'my_empty',
            '1',
            '-1',
            '0',
            '999',
            999) IS NOT FALSE
  AND c IN ('[2]',
            '[3, [4, 5]]',
            '2',
            '3',
            '999',
            999) IS FALSE;
+---+
| c |
+---+
|   |
| 1 |
+---+
(2 rows)

!ok

