#-------------------------------------------------------------------------
# TESTCASE: test_join TEST_ID: A4_B7
#-------------------------------------------------------------------------
SELECT t1.c_str,
       t1.c_long,
       c
FROM test_unnest t1
RIGHT JOIN
  (SELECT *
   FROM test_unnest,
        unnest(mv_to_array(c_mv_num_with_null_and_empty_val)) AS u(c)) t2 ON t2.c=cast(t1.c_long AS char)
WHERE t1.c_long IN (0,
                    1,
                    2,
                    999)
  AND t2.c_long IN (0,
                    1,
                    2,
                    999);
+-------+--------+---+
| c_str | c_long | c |
+-------+--------+---+
|       |      1 | 1 |
|       |      1 | 1 |
|       |      1 | 1 |
| str2  |      2 | 2 |
| str2  |      2 | 2 |
| str2  |      2 | 2 |
+-------+--------+---+
(6 rows)

!ok

