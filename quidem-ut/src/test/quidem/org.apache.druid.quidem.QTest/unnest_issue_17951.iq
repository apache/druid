#-------------------------------------------------------------------------
# This test currently doesn't pass because the native query plan generated
# uses an incorrect virtual column expression for the unnest part.
#-------------------------------------------------------------------------
WITH cte1 AS (
  SELECT
    a_int,
    TIME_FLOOR(__time, 'PT5M') AS floored_time,
    ARRAY_LENGTH(a_int) AS a_int_len
  FROM test_unnest
  GROUP BY 1, 2
),
cte2 AS (
  SELECT
    a_int,
    ARRAY_AGG(ARRAY[floored_time, a_int_len]) AS pair
  FROM cte1
  GROUP BY 1
)
SELECT
  tr.a_int,
  tv
FROM cte2 AS tr
CROSS JOIN UNNEST(pair) AS tv;
