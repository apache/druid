#-------------------------------------------------------------------------
# TESTCASE: test_order_by TEST_ID: A4_sum_count_W22_G1_Q2
#-------------------------------------------------------------------------
SELECT server_ip,
       client_ip,
       timezone,
       cast(time_floor(__time, 'PT12H') AS char) t,
       sum(length(timezone)) OVER w "sum_val",
       count(timezone) OVER w cnt_val
FROM
  (SELECT server_ip,
          client_ip,
          timezone,
          time_floor(__time, 'PT12H') __time,
          count(*)
   FROM test_win
   GROUP BY server_ip,
            client_ip,
            timezone,
