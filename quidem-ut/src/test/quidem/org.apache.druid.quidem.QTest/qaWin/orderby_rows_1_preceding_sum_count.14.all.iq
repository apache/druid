#-------------------------------------------------------------------------
# TESTCASE: test_order_by TEST_ID: A8_sum_count_W9_G1_Q2
#-------------------------------------------------------------------------
SELECT server_ip,
       client_ip,
       timezone,
       cast(time_floor(__time, 'PT12H') AS char) t,
       sum(length(timezone)) OVER w "sum_val",
       count(timezone) OVER w cnt_val
FROM
  (SELECT server_ip,
          client_ip,
          timezone,
          time_floor(__time, 'PT12H') __time,
          count(*)
   FROM test_win
   GROUP BY server_ip,
            client_ip,
            timezone,
            time_floor(__time, 'PT12H'))
WHERE client_ip IN ('107.13.54.103',
                    '99.9.55.22') WINDOW w AS (PARTITION BY client_ip
                                               ORDER BY server_ip,
                                                        cast(length(server_ip) AS DOUBLE)+0.123 ASC,
                                                        timezone,
                                                        time_floor(__time, 'PT12H') ROWS BETWEEN 1 PRECEDING AND UNBOUNDED FOLLOWING);
