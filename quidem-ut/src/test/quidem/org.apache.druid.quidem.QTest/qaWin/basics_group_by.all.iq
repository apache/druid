       client_ip,
       timezone,
       cast(time_floor(__time, 'PT12H') AS char) t,
       row_number() OVER (PARTITION BY client_ip
                          ORDER BY server_ip, timezone, time_floor(__time, 'PT12H')) AS c
FROM
  (SELECT server_ip,
          client_ip,
          timezone,
          time_floor(__time, 'PT12H') __time,
