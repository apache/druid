#-------------------------------------------------------------------------
# TESTCASE: test_order_by TEST_ID: A8_first_last_W21_G2_Q2
#-------------------------------------------------------------------------
SELECT server_ip,
       client_ip,
       timezone,
       cast(time_floor(__time, 'PT12H') AS char) t,
       first_value(timezone) OVER w first_val,
       last_value(timezone) OVER w last_val
FROM test_win
WHERE client_ip IN ('107.13.54.103',
                    '99.9.55.22')
GROUP BY server_ip,
         client_ip,
         timezone,
         time_floor(__time, 'PT12H') WINDOW w AS (PARTITION BY client_ip ROWS BETWEEN CURRENT ROW AND UNBOUNDED FOLLOWING);
