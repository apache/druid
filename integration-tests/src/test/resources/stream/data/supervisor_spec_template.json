{
  "type": "%%STREAM_TYPE%%",
  "dataSchema": {
    "dataSource": "%%DATASOURCE%%",
    "parser": %%PARSER%%,
    "timestampSpec": {
      "column": "timestamp",
      "format": "auto"
    },
    "dimensionsSpec": {
      "dimensions": %%DIMENSIONS%%,
      "dimensionExclusions": [],
      "spatialDimensions": []
    },
    "metricsSpec": [
      {
        "type": "count",
        "name": "count"
      },
      {
        "type": "doubleSum",
        "name": "added",
        "fieldName": "added"
      },
      {
        "type": "doubleSum",
        "name": "deleted",
        "fieldName": "deleted"
      },
      {
        "type": "doubleSum",
        "name": "delta",
        "fieldName": "delta"
      }
    ],
    "granularitySpec": {
      "type": "uniform",
      "segmentGranularity": "MINUTE",
      "queryGranularity": "NONE"
    }
  },
  "tuningConfig": {
    "type": "%%STREAM_TYPE%%",
    "intermediatePersistPeriod": "PT30S",
    "maxRowsPerSegment": 5000000,
    "maxRowsInMemory": 500000
  },
  "ioConfig": {
    "%%TOPIC_KEY%%": "%%TOPIC_VALUE%%",
    "%%STREAM_PROPERTIES_KEY%%": %%STREAM_PROPERTIES_VALUE%%,
    "taskCount": 2,
    "replicas": 1,
    "taskDuration": "PT120S",
    "%%USE_EARLIEST_KEY%%": true,
    "inputFormat" : %%INPUT_FORMAT%%
  }
}
