type: "operatorValidation"

sql: |
  SELECT
    dim2,
    SUM(m1) OVER (PARTITION BY dim2 order by dim1 ) cc
  FROM druid.foo

expectedOperators:
  - type: "naiveSort"
    columns:
    - column: dim2
      direction: ASC
    - column: dim1
      direction: ASC
  - type: "naivePartition"
    partitionColumns: ["dim2"]
  - type: "window"
    processor:
      type: "framedAgg"
      frame: { peerType: "ROWS", lowUnbounded: true, lowOffset: 0, uppUnbounded: false, uppOffset: 0 }
      aggregations:
        - { type: "doubleSum", name: "w0", fieldName: "m1" }

expectedResults:
  - [ "", 1.0 ]
