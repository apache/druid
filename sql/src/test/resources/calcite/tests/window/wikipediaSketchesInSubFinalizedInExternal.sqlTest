type: "operatorValidation"

sql: |
  select the_day, PAIR_LEFT(daily_metric_1), PAIR_RIGHT(daily_metric_1), PAIR_LEFT(weekly_metric_1), PAIR_RIGHT(weekly_metric_1)
  FROM (
    SELECT
      FLOOR (__time TO HOUR) AS the_day,
      STRING_EARLIEST(__time, user, 36) AS daily_metric_1,
      STRING_EARLIEST(STRING_EARLIEST(__time, user, 36), 36) OVER (ORDER BY CAST(FLOOR (__time TO HOUR) AS BIGINT) RANGE BETWEEN 7 PRECEDING AND 0 FOLLOWING) AS weekly_metric_1
    FROM "wikipedia"
    GROUP BY 1
  )

