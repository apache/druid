type: "operatorValidation"

sql: |
  SELECT
    dim1,
    dim1 || dim1,
    length(dim1 || dim1),
    MIN(length(dim1 || dim1)) OVER ()
  FROM foo

expectedOperators:
  - { type: "naivePartition", partitionColumns: [ ] }
  - type: "window"
    processor:
      type: "framedAgg"
      frame: { peerType: "ROWS", lowUnbounded: true, lowOffset: 0, uppUnbounded: true, uppOffset: 0 }
      aggregations:
        - { type: "doubleMin", name: "w0", fieldName: "_v0" }
        - { type: "longMin", name: "w1", fieldName: "v1" }

expectedResults:
  - ["_",null,11]
  - ["_10.1",null,11]
  - ["_2",null,11]
  - ["_1",null,11]
  - ["_def",null,11]
  - ["_abc",null,11]
