{
  "type": "index_parallel",
  "spec": {
    "ioConfig": {
      "type": "index_parallel",
      "inputSource" : {
        "type" : "local",
        "baseDir" : "sql/src/test/quidem/qatests/qaTpch/data",
        "filter" : "nation.tbl.gz"
      },
      "inputFormat": { "type": "tsv", "columns": ["n_nationkey", "n_name", "n_regionkey", "n_comment"], "findColumnsFromHeader": false, "delimiter": "|" },
      "appendToExisting": false
    },
    "tuningConfig": {
      "type": "index_parallel",
      "partitionsSpec": {
        "type": "dynamic",
        "maxRowsPerSegment": 100,
        "maxTotalRows": 100
      },
      "maxNumConcurrentSubTasks": 2,
      "indexSpec": {
        "stringDictionaryEncoding": {
          "type": "frontCoded",
          "bucketSize": 4,
          "formatVersion": 1
        }
      }
    },
    "dataSchema": {
      "dataSource": "nation",
      "granularitySpec": {
        "segmentGranularity": "year",
        "queryGranularity": null,
        "rollup": false
      },
      "timestampSpec": {
        "column": "timestamp",
        "format": "auto",
	"missingValue": "2025-01-01T00:00:00.000"
      },
      "dimensionsSpec": {
        "dimensions": [{"name": "n_nationkey", "type": "long"}, {"name": "n_name", "type": "string"}, {"name": "n_regionkey", "type": "long"}, {"name": "n_comment", "type": "string"}]
      }
    }
  }
}


