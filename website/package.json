{
  "license": "Apache-2.0",
  "scripts": {
    "examples": "./node_modules/.bin/docusaurus-examples",
    "start": "./node_modules/.bin/docusaurus-start",
    "build": "npm run compile-scss && ./node_modules/.bin/docusaurus-build && node script/do-redirects.js && node script/fix-docsearch.js",
    "publish-gh-pages": "./node_modules/.bin/docusaurus-publish",
    "write-translations": "./node_modules/.bin/docusaurus-write-translations",
    "version": "./node_modules/.bin/docusaurus-version",
    "rename-version": "./node_modules/.bin/docusaurus-rename-version",
    "compile-scss": "./node_modules/.bin/node-sass scss/custom.scss > static/css/custom.css",
    "link-lint": "npm run build && node script/link-lint.js",
    "spellcheck": "./node_modules/.bin/mdspell --en-us --ignore-numbers --report '../docs/**/*.md' || (./script/notify-spellcheck-issues && false)"
  },
  "engines": {
    "node": ">=16"
  },
  "volta": {
    "node": "16.17.0",
    "npm": "8.15.0"
  },
  "devDependencies": {
    "docusaurus": "^1.14.7",
    "markdown-spellcheck": "^1.3.1",
    "node-sass": "^7.0.0"
  },
  "dependencies": {
    "fast-glob": "^3.2.2",
    "fs-extra": "^8.1.0",
    "replace-in-file": "^4.3.1"
  }
}
